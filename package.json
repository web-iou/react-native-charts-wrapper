{
  "name": "@see_you/react-native-charts-wrapper",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-iou/react-native-charts-wrapper.git"
  },
  "version": "1.0.2",
  "description": "A react-native charts support both android and iOS.",
  "author": "chenxiang",
  "license": "MIT",
  "nativePackage": true,
  "homepage": "https://github.com/web-iou/react-native-charts-wrapper",
  "keywords": [
    "react native",
    "chart",
    "android",
    "MPAndroidChart",
    "ios",
    "Charts"
  ],
  "main": "./lib/module/index.js",
  "exports": {
    ".": {
      "source": "./src/index.js",
      "default": "./lib/module/index.js",
      "types": "./lib/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepare": "bob build && cp index.d.ts lib/index.d.ts"
  },
  "files": [
    "android/src",
    "android/build.gradle",
    "android/gradle.properties",
    "ios/**/*.h",
    "ios/**/*.m",
    "ios/**/*.swift",
    "lib",
    "index.js",
    "react-native-charts-wrapper.podspec",
    "ios/ReactNativeCharts/ReactNativeCharts.xcodeproj/project.pbxproj"
  ],
  "dependencies": {
    "react-native": "0.78.2"
  },
  "react-native-builder-bob": {
    "output": "lib",
    "source": "src",
    "targets": [
      [
        "module",
        {
          "esm": true,
          "sourceMaps": false
        }
      ]
    ]
  },
  "devDependencies": {
    "@types/react": "19.0.0",
    "react-native-builder-bob": "^0.40.13"
  },
  "types": "./lib/index.d.ts"
}
